{
  "background": {
    "scripts": [
  "global_files/browserDetect.js",
  "global_files/browser-polyfill.js",
  "services/UsefulFunctions.js",
  "services/WhiteListService.js",
  "services/NotificationService.js",
  "services/CleanupService.js",
	"background.js"
	]
  },
  "description": "Control Your Cookies! Auto Delete Unused Cookies From your Closed Tabs While Keeping The Ones You Want.",
  "homepage_url": "https://github.com/mrdokenny/Cookie-AutoDelete",
  "browser_action": {
    "default_icon": {    
      "48": "icons/icon_48.png"     
    },
    "default_title": "Cookie AutoDelete",
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "48": "icons/icon_48.png",     
    "128": "icons/icon_128.png"
  },
  
  "applications": {
    "gecko": {
      "id": "CookieAutoDelete@kennydo.com",
      "strict_min_version": "50.0"
    }
  },
  "options_ui": {
  	"page": "settings/settings.html",
  	"open_in_tab" : true
  },
  "permissions": [
    "activeTab",
    "cookies",
    "contextualIdentities",
    "<all_urls>",
    "tabs",
    "storage",
    "alarms",
    "notifications"
  ],
  "manifest_version": 2,
  "name": "Cookie AutoDelete",
  "version": "1.1.1rc1"
}
